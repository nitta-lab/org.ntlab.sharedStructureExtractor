            annotationType="org.ntlab.deltaExtractor.srcSideDeltaAnnotation"
            colorPreferenceKey="clcolor3"
            colorPreferenceValue="255,204,216"
            highlightPreferenceKey="clhighlight3"
            highlightPreferenceValue="true"
            icon="icons/deltaMarker.png"
            includeOnPreferencePage="true"
            label="SrcSideDeltaAnnotation"
            overviewRulerPreferenceKey="clruler3"
            overviewRulerPreferenceValue="true"
            textPreferenceKey="cltext3"
            textPreferenceValue="true"
            textStylePreferenceKey="clstyle3"
            textStylePreferenceValue="BOX"
            verticalRulerPreferenceKey="clvertical3"
            verticalRulerPreferenceValue="true">
      </specification>
      <specification
            annotationType="org.ntlab.deltaExtractor.dstSideDeltaAnnotation"
            colorPreferenceKey="clcolor4"
            colorPreferenceValue="153,255,255"
            highlightPreferenceKey="clhighlight4"
            highlightPreferenceValue="true"
            icon="icons/deltaMarker.png"
            includeOnPreferencePage="true"
            label="DstSideDeltaAnnotation"
            overviewRulerPreferenceKey="clruler4"
            overviewRulerPreferenceValue="true"
            textPreferenceKey="cltext4"
            textPreferenceValue="true"
            textStylePreferenceKey="clstyle4"
            textStylePreferenceValue="BOX"
            verticalRulerPreferenceKey="clvertical4"

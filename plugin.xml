<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.views">
      <category
            id="org.ntlab.deltaExtractor.category"
            name="TraceDebugger">
      </category>
      <view
            category="org.ntlab.deltaExtractor.category"
            class="org.ntlab.deltaExtractor.views.BreakPointView"
            icon="icons/view/breakpoint_view.png"
            id="org.ntlab.deltaExtractor.breakPointView"
            name="Breakpoints"
            restorable="true">
      </view>
      <view
            category="org.ntlab.deltaExtractor.category"
            class="org.ntlab.deltaExtractor.views.VariableView"
            icon="icons/view/variable_view.png"
            id="org.ntlab.deltaExtractor.variableView"
            name="Variables"
            restorable="true">
      </view>
      <view
            category="org.ntlab.deltaExtractor.category"
            class="org.ntlab.deltaExtractor.views.CallStackView"
            icon="icons/view/callstack_view.png"
            id="org.ntlab.deltaExtractor.callStackView"
            name="Call Stack"
            restorable="true">
      </view>
      <category
            id="org.ntlab.deltaExtractor.category2"
            name="TraceDebuggerRelatedDelta">
      </category>
      <view
            category="org.ntlab.deltaExtractor.category2"
            class="org.ntlab.deltaExtractor.views.BreakPointViewRelatedDelta"
            icon="icons/view/breakpoint_view.png"
            id="org.ntlab.deltaExtractor.breakPointViewRelatedDelta"
            name="Breakpoints"
            restorable="true">
      </view>
      <view
            category="org.ntlab.deltaExtractor.category2"
            class="org.ntlab.deltaExtractor.views.VariableViewRelatedDelta"
            icon="icons/view/variable_view.png"
            id="org.ntlab.deltaExtractor.variableViewRelatedDelta"
            name="Variables"
            restorable="true">
      </view>
      <view
            category="org.ntlab.deltaExtractor.category2"
            class="org.ntlab.deltaExtractor.views.CallStackViewRelatedDelta"
            icon="icons/view/callstack_view.png"
            id="org.ntlab.deltaExtractor.callStackViewRelatedDelta"
            name="Call Stack"
            restorable="true">
      </view>
      <view
            category="org.ntlab.deltaExtractor.category2"
            class="org.ntlab.deltaExtractor.views.TracePointsRegisterView"
            icon="icons/view/tracepoints_view.png"
            id="org.ntlab.deltaExtractor.tracePointsRegisterView"
            name="Bookmarked Execution Points"
            restorable="true">
      </view>
      <view
            allowMultiple="true"
            category="org.ntlab.deltaExtractor.category2"
            class="org.ntlab.deltaExtractor.views.DeltaMarkerView"
            icon="icons/view/deltamarker_view.png"
            id="org.ntlab.deltaExtractor.deltaMarkerView"
            name="Extracted Delta"
            restorable="true">
      </view>
      <view
            allowMultiple="true"
            category="org.ntlab.deltaExtractor.category2"
            class="org.ntlab.deltaExtractor.views.CallTreeView"
            icon="icons/view/calltree_view.png"
            id="org.ntlab.deltaExtractor.callTreeView"
            name="Current Execution Point in Call Tree"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="org.ntlab.deltaExtractor.views.DeltaExtractorPerspective"
            id="org.ntlab.deltaExtractor.deltaExtractorPerspective"
            name="Trace Debugger (Forward)">
      </perspective>
      <perspective
            class="org.ntlab.deltaExtractor.views.DeltaExtractorPerspectiveRelatedDelta"
            id="org.ntlab.deltaExtractor.deltaExtractorPerspectiveRelatedDelta"
            name="Trace Dubugger (Reverse)">
      </perspective>
   </extension>
   <extension
         point="org.ntlab.traceAnalysisPlatform.additionalClasspaths">
      <classpath
            class="org.ntlab.deltaExtractor.analyzerProvider.DeltaExtractionAnalyzerLaunchConfiguration">
      </classpath>
   </extension>
   <extension
         id="org.ntlab.deltaExtractor.bottomDeltaMarker"
         name="BottomDeltaMarker"
         point="org.eclipse.core.resources.markers">
      <persistent
            value="false">
      </persistent>
      <super
            type="org.eclipse.core.resources.textmarker">
      </super>
   </extension>
   <extension
         id="org.ntlab.deltaExtractor.coordinatorDeltaMarker"
         name="CoordinatorDeltaMarker"
         point="org.eclipse.core.resources.markers">
      <persistent
            value="false">
      </persistent>
      <super
            type="org.eclipse.core.resources.textmarker">
      </super>
   </extension>
   <extension
         id="org.ntlab.deltaExtractor.srcSideDeltaMarker"
         name="SrcSideDeltaMarker"
         point="org.eclipse.core.resources.markers">
      <persistent
            value="false">
      </persistent>
      <super
            type="org.eclipse.core.resources.textmarker">
      </super>
   </extension>
   <extension
         id="org.ntlab.deltaExtractor.dstSideDeltaMarker"
         name="DstSideDeltaMarker"
         point="org.eclipse.core.resources.markers">
      <persistent
            value="false">
      </persistent>
      <super
            type="org.eclipse.core.resources.textmarker">
      </super>
   </extension>
   <extension
         id="org.ntlab.deltaExtractor.currentMarker"
         name="CurrentMarker"
         point="org.eclipse.core.resources.markers">
      <persistent
            value="false">
      </persistent>
      <super
            type="org.eclipse.core.resources.textmarker">
      </super>
   </extension>
   <extension
         point="org.eclipse.ui.ide.markerImageProviders">
      <imageprovider
            icon="icons/deltaMarker.png"
            id="BottomDeltaMarker"
            markertype="org.ntlab.deltaExtractor.bottomDeltaMarker">
      </imageprovider>
      <imageprovider
            icon="icons/deltaMarker.png"
            id="CoordinatorDeltaMarker"
            markertype="org.ntlab.deltaExtractor.coordinatorDeltaMarker">
      </imageprovider>
      <imageprovider
            icon="icons/deltaMarker.png"
            id="SrcSideDeltaMarker"
            markertype="org.ntlab.deltaExtractor.srcSideDeltaMarker">
      </imageprovider>
      <imageprovider
            icon="icons/deltaMarker.png"
            id="DstSideDeltaMarker"
            markertype="org.ntlab.deltaExtractor.dstSideDeltaMarker">
      </imageprovider>
      <imageprovider
            icon="icons/currentMarker.png"
            id="CurrentMarker"
            markertype="org.ntlab.deltaExtractor.currentMarker">
      </imageprovider>
   </extension>
   <extension
         point="org.eclipse.ui.editors.annotationTypes">
      <type
            markerSeverity="0"
            markerType="org.ntlab.deltaExtractor.bottomDeltaMarker"
            name="org.ntlab.deltaExtractor.bottomDeltaAnnotation"
            super="org.eclipse.ui.workbench.texteditor.info">
      </type>
      <type
            markerSeverity="0"
            markerType="org.ntlab.deltaExtractor.coordinatorDeltaMarker"
            name="org.ntlab.deltaExtractor.coordinatorDeltaAnnotation"
            super="org.eclipse.ui.workbench.texteditor.info">
      </type>
      <type
            markerSeverity="0"
            markerType="org.ntlab.deltaExtractor.srcSideDeltaMarker"
            name="org.ntlab.deltaExtractor.srcSideDeltaAnnotation"
            super="org.eclipse.ui.workbench.texteditor.info">
      </type>
      <type
            markerSeverity="0"
            markerType="org.ntlab.deltaExtractor.dstSideDeltaMarker"
            name="org.ntlab.deltaExtractor.dstSideDeltaAnnotation"
            super="org.eclipse.ui.workbench.texteditor.info">
      </type>
      <type
            markerSeverity="0"
            markerType="org.ntlab.deltaExtractor.currentMarker"
            name="org.ntlab.deltaExtractor.currentAnnotation"
            super="org.eclipse.ui.workbench.texteditor.info">
      </type>
   </extension>
   <extension
         point="org.eclipse.ui.editors.markerAnnotationSpecification">
      <specification
            annotationType="org.ntlab.deltaExtractor.bottomDeltaAnnotation"
            colorPreferenceKey="clcolor"
            colorPreferenceValue="255,204,153"
            highlightPreferenceKey="clhighlight"
            highlightPreferenceValue="true"
            icon="icons/deltaMarker.png"
            includeOnPreferencePage="true"
            label="BottomDeltaAnnotation"
            overviewRulerPreferenceKey="clruler"
            overviewRulerPreferenceValue="true"
            textPreferenceKey="cltext"
            textPreferenceValue="true"
            textStylePreferenceKey="clstyle"
            textStylePreferenceValue="BOX"
            verticalRulerPreferenceKey="clvertical"
            verticalRulerPreferenceValue="true">
      </specification>
      <specification
            annotationType="org.ntlab.deltaExtractor.coordinatorDeltaAnnotation"
            colorPreferenceKey="clcolor2"
            colorPreferenceValue="255,255,153"
            highlightPreferenceKey="clhighlight2"
            highlightPreferenceValue="true"
            icon="icons/deltaMarker.png"
            includeOnPreferencePage="true"
            label="CoordinatorDeltaAnnotation"
            overviewRulerPreferenceKey="clruler2"
            overviewRulerPreferenceValue="true"
            textPreferenceKey="cltext2"
            textPreferenceValue="true"
